@model LibGuides.Common.LibGuidesApiResponse
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
        <div class="pt-4 mt-5">
            <div class="row mrg-0">
                <div class="col-12 p-l-25 p-r-25">
                    <div class="ibox mb-0  mb-5">
                        <div class="ibox-content">
                           

                            <div class="">
                                <table id="" class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col" class="text-left" width="15%">Id</th>
                                            <th scope="col" class="text-left">Name</th>
                                            <th scope="col" class="text-left">FirstName</th>
                                            <th scope="col" class="text-left">Status</th>
                                            <th width="10%"></th>
                                        </tr>
                                    </thead>
                                    <tbody id="view-presc-body" class="ticket-color">
                                        @foreach (var item in Model.Result)
                                        {
                                            <tr>
                                                <td class="text-left">@item.Id</td>
                                                <td>
                                                    <span class="text-left">@item.Name</span>
                                                </td>
                                                <td scope="row" class="text-left">@item.Owner.FirstName</td>
                                                <td class="text-left">@item.Status</td>
                                                <td class="text-right">
                                                    <a href="/Customer/HelpCenterTicketDetails?id=@item.Id" class="tooltiped" data-toggle="tooltip" data-placement="top" title="Response to Open Ticket">
                                                        <img width="25" src="data:image/svg+xml;utf8;base64,">
                                                    </a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    
</div>